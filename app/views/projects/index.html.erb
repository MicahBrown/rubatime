<h1 class="title">Projects</h1>

<p>
  <%= link_to "New Project", new_project_path %>
</p>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= link_to "Edit", edit_project_path(project) %></td>
      </tr>
    <% end %>
  </tbody>
</table>